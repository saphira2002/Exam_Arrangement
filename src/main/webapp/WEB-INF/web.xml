<web-app>
  <display-name>Archetype Created Web Application</display-name>

<servlet>
  <servlet-name>Login</servlet-name>
  <servlet-class>ds.Action</servlet-class>
</servlet>
  <servlet-mapping>
    <servlet-name>Login</servlet-name>
    <url-pattern>/action</url-pattern>
